# Wisc CS .bashrc.local

export PATH=$PATH:~/bin:/p/course/cs354-common/public/bin

# Shortcuts
alias 354="cd ~/private/cs354"
alias ch2="ls -l /p/course/cs354-common/public/spring12.handin/betz/a2"
alias handin2="/s/handin/bin/handin -c cs354-1 -a a2 -d ."

# Aliases to cc
function co() {
  cc -o $1 $1.c
}

function cr() {
  co $1 && $1
}

. ~/bin/dotfiles/zsh/git_aliases
