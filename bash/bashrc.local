# Wisc CS .bashrc.local

. ~/bin/dotfiles/bashrc

export PATH=$PATH:~/bin:/p/course/cs354-common/public/bin

# Shortcuts
alias 354="cd ~/private/cs354"
alias ch3="ls -l /p/course/cs354-common/public/spring12.handin/betz/a3"
alias handin3="/s/handin/bin/handin -c cs354-1 -a a3 -d ."

# Aliases to cc
function co() {
  cc -o $1 $1.c
}

function cr() {
  co $1 && $1
}
